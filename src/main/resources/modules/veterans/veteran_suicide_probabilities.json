        {
          "transition": "Terminal"
        }
      ]
    },
    "Baseline": {
      "type": "SetAttribute",
      "attribute": "probability_of_suicide",
      "direct_transition": "Ideation_Baseline",
      "value": 0.0003072
    },
    "Homelessness_Check": {
      "type": "Simple",
      "conditional_transition": [
        {
          "transition": "Increment_Counter_Homelessness",
          "condition": {
            "condition_type": "Attribute",
            "attribute": "homeless",
            "operator": "==",
            "value": true
          }
        },
        {
          "transition": "Hypertension_Check"
        }
      ]
    },
    "End_Counter": {
      "type": "Simple",
      "conditional_transition": [
        {
          "transition": "High Risk",
          "condition": {
            "condition_type": "And",
            "conditions": [
              {
                "condition_type": "Attribute",
                "attribute": "suicide_risk_factor_count",
                "operator": ">",
                "value": 30
              },
              {
                "condition_type": "Attribute",
                "attribute": "suicide_risk_factor_count",
                "operator": "<=",
                "value": 50
              }
            ]
