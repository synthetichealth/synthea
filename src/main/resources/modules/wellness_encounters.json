    },
    "Record_EGFR": {
      "type": "Observation",
      "category": "laboratory",
      "vital_sign": "EGFR",
      "codes": [
        {
          "system": "LOINC",
          "code": "33914-3",
          "display": "Glomerular filtration rate/1.73 sq M.predicted Volume Rate/Area in Serum or Plasma by Creatinine-based formula (MDRD)"
