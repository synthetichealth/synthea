    },
    "Symptom_Encounter": {
      "type": "Encounter",
      "encounter_class": "ambulatory",
      "reason": "Colorectal_Cancer_Symptom_2",
      "codes": [
        {
          "system": "SNOMED-CT",
          "code": "185345009",
          "display": "Encounter for symptom"
        }
      ],
      "direct_transition": "End_Symptom_Encounter"
    },
    "End_Symptom_Encounter" : {
      "type" : "EncounterEnd",
