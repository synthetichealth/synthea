            {
              "transition": "Redo Echo Encounter",
              "distribution": 0.25
            }
          ]
        }
      ]
    },
    "Severity Check": {
      "type": "Simple",
      "complex_transition": [
        {
          "condition": {
            "condition_type": "Age",
            "operator": "<=",
            "quantity": 44,
            "unit": "years",
            "value": 0
          },
          "distributions": [
            {
              "transition": "End Echo Encounter",
              "distribution": 0.005
            },
            {
              "transition": "Redo Echo Encounter",
              "distribution": 0.995
            }
          ]
        },
        {
          "condition": {
            "condition_type": "Age",
            "operator": "<=",
            "quantity": 54,
            "unit": "years",
            "value": 0
          },
          "distributions": [
            {
              "transition": "End Echo Encounter",
              "distribution": 0.03
            },
