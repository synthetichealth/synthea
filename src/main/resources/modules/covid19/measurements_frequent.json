    "Procalcitonin": {
      "type": "Observation",
      "category": "laboratory",
      "unit": "ng/mL",
      "codes": [
        {
          "system": "LOINC",
          "code": "33959-8",
          "display": "Procalcitonin [Mass/volume] in Serum or Plasma"
        }
      ],
      "direct_transition": "Terminal",
      "attribute": "covid19_procalcitonin"
    }
