    "ICU Check": {
      "type": "Simple",
      "conditional_transition": [
        {
          "transition": "Record O2",
          "condition": {
            "condition_type": "And",
            "conditions": [
              {
                "condition_type": "Attribute",
                "attribute": "covid19_icu_days",
                "operator": "is not nil"
              },
              {
                "condition_type": "Attribute",
                "attribute": "covid19_icu_days",
                "operator": ">=",
                "value": 0
              }
            ]
          }
        },
        {
          "transition": "Terminal"
        }
      ]
    },
    "Record O2": {
