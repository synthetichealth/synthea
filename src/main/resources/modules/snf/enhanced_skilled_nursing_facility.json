{
  "name": "Enhanced Skilled Nursing Facility with PFE",
  "remarks": [
    "Enhanced Skilled Nursing Facility module with integrated Personal Functioning and Engagement assessments.",
    "Extends the base SNF workflow with comprehensive PFE documentation.",
    "Includes realistic assessment scheduling and care planning integration.",
    "Based on PACIO PFE Implementation Guide requirements."
  ],
  "states": {
    "Initial": {
      "type": "Initial",
      "conditional_transition": [
        {
          "transition": "Transition_To_SNF_Reason",
          "condition": {
            "condition_type": "Attribute",
            "attribute": "snf_reason",
            "operator": "is nil"
          }
        },
        {
          "transition": "SNF_Admission"
        }
      ]
    },
    
    "SNF_Admission": {
      "type": "Encounter",
      "encounter_class": "snf",
      "codes": [
        {
          "system": "SNOMED-CT",
          "code": "32485007",
          "display": "Hospital admission (procedure)"
        }
      ],
      "conditional_transition": [
        {
          "transition": "Determine_LOS",
          "condition": {
            "condition_type": "Or",
            "conditions": [
              {
                "condition_type": "Attribute",
                "attribute": "snf_days",
                "operator": "is nil"
              },
              {
                "condition_type": "Attribute",
                "attribute": "snf_days",
                "operator": "<=",
                "value": 0
              }
            ]
          }
        },
        {
          "transition": "History_Physical_Exam"
        }
      ],
      "reason": "snf_reason"
    },

    "Determine_LOS": {
      "type": "SetAttribute",
      "attribute": "snf_days",
      "distribution": {
        "kind": "EXPONENTIAL",
        "round": true,
        "parameters": {
          "mean": 18.7
        }
      },
      "conditional_transition": [
        {
          "transition": "Max_Length_of_Stay",
          "condition": {
            "condition_type": "Attribute",
            "attribute": "snf_days",
            "operator": ">",
            "value": 100
          }
        },
        {
          "transition": "History_Physical_Exam"
        }
      ]
    },

    "Max_Length_of_Stay": {
      "type": "SetAttribute",
      "attribute": "snf_days",
      "value": 100,
      "direct_transition": "History_Physical_Exam"
    },

    "History_Physical_Exam": {
      "type": "Procedure",
      "codes": [
        {
          "system": "SNOMED-CT",
          "code": "63332003",
          "display": "History AND physical examination (procedure)"
        }
      ],
      "direct_transition": "Initial_Patient_Assessment"
    },

    "Initial_Patient_Assessment": {
      "type": "Procedure",
      "codes": [
        {
          "system": "SNOMED-CT",
          "code": "315639002",
          "display": "Initial patient assessment (procedure)"
        }
      ],
      "direct_transition": "Launch_PFE_Assessments"
    },

    "Launch_PFE_Assessments": {
      "type": "CallSubmodule",
      "submodule": "snf/snf_pfe_assessments",
      "direct_transition": "Facility_Wheelchair"
    },

    "Facility_Wheelchair": {
      "type": "Device",
      "code": {
        "system": "SNOMED-CT",
        "code": "228869008",
        "display": "Manual wheelchair (physical object)"
      },
      "direct_transition": "Development_of_Individualized_Care_Plan",
      "assign_to_attribute": "snf_wheelchair"
    },

    "Development_of_Individualized_Care_Plan": {
      "type": "Procedure",
      "codes": [
        {
          "system": "SNOMED-CT",
          "code": "370789001",
          "display": "Development of individualized plan of care (procedure)"
        }
      ],
      "direct_transition": "Begin_Day"
    },

    "Begin_Day": {
      "type": "Counter",
      "attribute": "snf_days",
      "action": "decrement",
      "amount": 1,
      "direct_transition": "Nursing"
    },

    "Nursing": {
      "type": "Procedure",
      "codes": [
        {
          "system": "SNOMED-CT",
          "code": "410401003",
          "display": "Nursing care/supplementary surveillance (regime/therapy)"
        }
      ],
      "direct_transition": "Physical_Therapy_Check"
    },

    "Physical_Therapy_Check": {
      "type": "Simple",
      "distributed_transition": [
        {
          "distribution": 0.15,
          "transition": "Physical_Therapy_with_Assessment"
        },
        {
          "distribution": 0.85,
          "transition": "Occupational_Therapy_Check"
        }
      ]
    },

    "Physical_Therapy_with_Assessment": {
      "type": "Procedure",
      "codes": [
        {
          "system": "SNOMED-CT",
          "code": "91251008",
          "display": "Physical therapy procedure (regime/therapy)"
        }
      ],
      "range": {
        "high": 60,
        "low": 45,
        "unit": "minutes"
      },
      "direct_transition": "PT_Mobility_Observation"
    },

    "PT_Mobility_Observation": {
      "type": "Observation",
      "category": "survey",
      "codes": [
        {
          "system": "LOINC",
          "code": "95011-3",
          "display": "Walk in room - functional ability during assessment period"
        }
      ],
      "value_code": {
        "system": "LOINC",
        "code": "LA30910-6",
        "display": "Setup help only"
      },
      "direct_transition": "Occupational_Therapy_Check"
    },

    "Occupational_Therapy_Check": {
      "type": "Simple",
      "distributed_transition": [
        {
          "distribution": 0.12,
          "transition": "Occupational_Therapy_with_Assessment"
        },
        {
          "distribution": 0.88,
          "transition": "Speech_Language_Therapy_Check"
        }
      ]
    },

    "Occupational_Therapy_with_Assessment": {
      "type": "Procedure",
      "codes": [
        {
          "system": "SNOMED-CT",
          "code": "84478008",
          "display": "Occupational therapy (regime/therapy)"
        }
      ],
      "duration": {
        "low": 30,
        "high": 60,
        "unit": "minutes"
      },
      "direct_transition": "OT_ADL_Observation"
    },

    "OT_ADL_Observation": {
      "type": "Observation",
      "category": "survey",
      "codes": [
        {
          "system": "LOINC",
          "code": "95019-6",
          "display": "Eating - functional ability during assessment period"
        }
      ],
      "value_code": {
        "system": "LOINC",
        "code": "LA30910-6",
        "display": "Setup help only"
      },
      "direct_transition": "Speech_Language_Therapy_Check"
    },

    "Speech_Language_Therapy_Check": {
      "type": "Simple",
      "distributed_transition": [
        {
          "distribution": 0.08,
          "transition": "Speech_Language_Therapy_with_Assessment"
        },
        {
          "distribution": 0.92,
          "transition": "NTA_Check"
        }
      ]
    },

    "Speech_Language_Therapy_with_Assessment": {
      "type": "Procedure",
      "codes": [
        {
          "system": "SNOMED-CT",
          "code": "311555007",
          "display": "Speech and language therapy regime (regime/therapy)"
        }
      ],
      "duration": {
        "low": 30,
        "high": 60,
        "unit": "minutes"
      },
      "direct_transition": "SLP_Communication_Observation"
    },

    "SLP_Communication_Observation": {
      "type": "Observation",
      "category": "survey",
      "codes": [
        {
          "system": "LOINC",
          "code": "99858-3",
          "display": "Functional Communication Measure - Swallowing score [ASHA NOMS]"
        }
      ],
      "unit": "{score}",
      "range": {
        "low": 1,
        "high": 7
      },
      "direct_transition": "NTA_Check"
    },

    "NTA_Check": {
      "type": "Simple",
      "distributed_transition": [
        {
          "distribution": 0.25,
          "transition": "Professional_Services_Care"
        },
        {
          "distribution": 0.75,
          "transition": "Status_Change_Check"
        }
      ]
    },

    "Professional_Services_Care": {
      "type": "Procedure",
      "codes": [
        {
          "system": "SNOMED-CT",
          "code": "409023009",
          "display": "Professional / ancillary services care (regime/therapy)"
        }
      ],
      "direct_transition": "Status_Change_Check"
    },

    "Status_Change_Check": {
      "type": "Simple",
      "distributed_transition": [
        {
          "distribution": 0.02,
          "transition": "Care_Regimes_Assessment"
        },
        {
          "distribution": 0.98,
          "transition": "End_Day"
        }
      ]
    },

    "Care_Regimes_Assessment": {
      "type": "Procedure",
      "codes": [
        {
          "system": "SNOMED-CT",
          "code": "392091000",
          "display": "Care regimes assessment (procedure)"
        }
      ],
      "direct_transition": "End_Day"
    },

    "End_Day": {
      "type": "Delay",
      "distribution": {
        "kind": "EXACT",
        "parameters": {
          "value": 1
        }
      },
      "unit": "days",
      "conditional_transition": [
        {
          "transition": "Begin_Day",
          "condition": {
            "condition_type": "Attribute",
            "attribute": "snf_days",
            "operator": ">",
            "value": 0
          }
        },
        {
          "transition": "Predischarge_Assessment"
        }
      ]
    },

    "Predischarge_Assessment": {
      "type": "Procedure",
      "codes": [
        {
          "system": "SNOMED-CT",
          "code": "225386006",
          "display": "Pre-discharge assessment (procedure)"
        }
      ],
      "direct_transition": "Final_PFE_Assessment"
    },

    "Final_PFE_Assessment": {
      "type": "Procedure",
      "codes": [
        {
          "system": "SNOMED-CT",
          "code": "273542005",
          "display": "Activities of daily living assessment (procedure)"
        }
      ],
      "duration": {
        "low": 30,
        "high": 45,
        "unit": "minutes"
      },
      "direct_transition": "Discharge_Mobility_Status"
    },

    "Discharge_Mobility_Status": {
      "type": "Observation",
      "category": "survey",
      "codes": [
        {
          "system": "LOINC",
          "code": "95012-1",
          "display": "Transfer - functional ability during assessment period"
        }
      ],
      "value_code": {
        "system": "LOINC",
        "code": "LA30909-8",
        "display": "Independent"
      },
      "direct_transition": "Discharge_From_SNF"
    },

    "Discharge_From_SNF": {
      "type": "Procedure",
      "codes": [
        {
          "system": "SNOMED-CT",
          "code": "308283009",
          "display": "Discharge from hospital (procedure)"
        }
      ],
      "direct_transition": "Return_Wheelchair"
    },

    "Return_Wheelchair": {
      "type": "DeviceEnd",
      "referenced_by_attribute": "snf_wheelchair",
      "direct_transition": "End_Encounter"
    },

    "End_Encounter": {
      "type": "EncounterEnd",
      "direct_transition": "Clear_SNF_Reason"
    },

    "Clear_SNF_Reason": {
      "type": "SetAttribute",
      "attribute": "snf_reason",
      "direct_transition": "Clear_PFE_Attributes"
    },

    "Clear_PFE_Attributes": {
      "type": "SetAttribute",
      "attribute": "snf_pfe_admission_complete",
      "direct_transition": "Terminal"
    },

    "Transition_To_SNF_Reason": {
      "type": "SetAttribute",
      "attribute": "snf_reason",
      "direct_transition": "SNF_Admission",
      "value_code": {
        "system": "SNOMED-CT",
        "code": "25675004",
        "display": "Patient transfer to skilled nursing facility (procedure)"
      }
    },

    "Terminal": {
      "type": "Terminal"
    }
  },
  "gmf_version": 2
}