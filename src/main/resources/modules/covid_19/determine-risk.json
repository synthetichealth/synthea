                                      {
                                        "condition_type": "Or",
                                        "conditions": [
                                          {
                                            "condition_type": "Attribute",
                                            "attribute": "chf",
                                            "operator": "is not nil"
                                          },
                                          {
                                            "condition_type": "Or",
                                            "conditions": [
                                              {
                                                "condition_type": "Attribute",
                                                "attribute": "homelessness_category",
                                                "operator": "==",
                                                "value": "chronic"
                                              },
                                              {
                                                "condition_type": "Or",
