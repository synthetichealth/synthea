      "type": "Initial",
      "remarks": [
        "======================================================================",
        " INCIDENCE                                                            ",
        "======================================================================",
        " | Arthritis Type | Prevalence | Ratio F:M | ",
        " ------------------------------------------- ",
        " | Fibromyalgia   |   0.01670  |    7:1    | ",
        " ------------------------------------------- "
      ],
      "complex_transition": [
        {
          "condition": {
            "condition_type": "Gender",
            "gender": "M"
          },
          "distributions": [
            {
              "distribution": 0.0020875,
              "transition": "Delay_Until_Fibromyalgia"
            },
            {
              "distribution": 0.9989125,
              "transition": "Terminal"
            }
          ]
        },
