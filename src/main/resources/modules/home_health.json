    },
    "Check for End": {
      "type": "Simple",
      "conditional_transition": [
        {
          "transition": "Finish Day",
          "condition": {
            "condition_type": "Attribute",
            "attribute": "home_health_days",
            "operator": ">",
            "value": 0
          }
        },
        {
          "transition": "Patient_Discharge"
        }
      ]
    },
    "Finish Day": {
      "type": "EncounterEnd",
      "conditional_transition": [
        {
          "transition": "Delay",
          "condition": {
            "condition_type": "Attribute",
            "attribute": "home_health_days",
            "operator": ">",
            "value": 0
          }
        },
        {
          "transition": "Patient_Discharge"
        }
