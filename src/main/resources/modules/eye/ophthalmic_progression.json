      },
      "direct_transition": "Check_Edema_for_VA"
    },
    "Set_Stage_Code_2": {
      "type": "SetAttribute",
      "attribute": "diabetic_retinopathy_stage_code",
      "value_code": {
        "system": "LOINC",
        "code": "LA18645-4",
        "display": "Moderate non-proliferative retinopathy"
      },
      "direct_transition": "Check_Edema_for_VA"
    },
    "Set_Stage_Code_3": {
      "type": "SetAttribute",
      "attribute": "diabetic_retinopathy_stage_code",
      "value_code": {
        "system": "LOINC",
        "code": "LA18646-2",
        "display": "Severe non-proliferative retinopathy"
      },
      "direct_transition": "Check_Edema_for_VA"
    },
    "Set_Stage_Code_4": {
      "type": "SetAttribute",
      "attribute": "diabetic_retinopathy_stage_code",
      "value_code": {
        "system": "LOINC",
        "code": "LA18648-8",
        "display": "Proliferative retinopathy"
      },
