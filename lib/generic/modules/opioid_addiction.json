      "target_encounter" : "Enter_Directed_Use_Encounter1",
      "codes" : [{
          "system" : "RxNorm",
          "code" : "1310197",
          "display" : "Acetaminophen 300 MG / HYDROcodone Bitartrate 5 MG [Vicodin]"
